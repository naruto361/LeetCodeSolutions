class Solution {
public:
    int numSubarrayBoundedMax(vector<int>& nums, int left, int right) {
        int ans=0,start=0,cnt=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]>right) start=i+1,cnt=0;
            else if(nums[i]<left)
            {
                ans+=cnt;
            }
            else // b/w left and right
            {
                cnt=i-start+1;
                ans+=cnt;
            }
        }
        return ans;
    }
};
