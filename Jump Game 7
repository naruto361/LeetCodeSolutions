class Solution {
public:
    bool canReach(string s, int minJump, int maxJump) {
        int n=s.length();
        vector<bool> dp(n,false);
        dp[0]=true;
        int sum=0;
        for(int i=1;i<n;i++)
        {
            if(i>=minJump && dp[i-minJump]) sum++;
            if(i>maxJump && dp[i-maxJump-1]) sum--;
            if(sum>0 && s[i]=='0') dp[i]=true;
        }
        return dp[n-1];
    }
};
